# Environment Configuration
# Set to 'dev' or 'prod' to determine which environment to deploy to
ENV=dev

# Stripe Configuration
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Admin API Configuration
ADMIN_API_URL=https://api-dev.vexa.ai
ADMIN_API_TOKEN=your_admin_api_token_here

# Portal Return URL (use your Cloud Run webapp URL)
PORTAL_RETURN_URL=https://dev-webapp-xxxxx-uc.a.run.app

# NextAuth Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=https://dev-webapp-xxxxx-uc.a.run.app

# Google OAuth Configuration (Required for Google login)
GOOGLE_CLIENT_ID=your_google_client_id_here
GOOGLE_CLIENT_SECRET=your_google_client_secret_here

# Service Ports (optional - defaults shown)
WEBAPP_HOST_PORT=3000
BILLING_HOST_PORT=19000

# Cloudflare (optional, for custom domains)
# Get token from: https://dash.cloudflare.com/profile/api-tokens
CLOUDFLARE_TOKEN=your_cloudflare_token_here

# Analytics Configuration
# Google Analytics 4 Measurement ID (starts with "G-")
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# Umami Analytics Website ID (optional, privacy-focused analytics)
NEXT_PUBLIC_UMAMI_WEBSITE_ID=your_umami_website_id_here


